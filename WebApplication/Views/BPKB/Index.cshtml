                @Html.DisplayFor(modelItem => item.faktur_no)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.faktur_date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.location_id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.police_no)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.bpkb_date_in)
            </td>
